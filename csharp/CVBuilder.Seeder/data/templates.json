[
  {
    "id": "classic",
    "name": "Classic",
    "description": "Traditional one-column layout with elegant serif typography. Perfect for corporate and academic roles.",
    "tags": ["Traditional", "Corporate", "One-Column"],
    "htmlContent": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>{{name}} - Resume</title>\n  <style>\n    * {\n      margin: 0;\n      padding: 0;\n      box-sizing: border-box;\n    }\n    \n    .classic-cv {\n      max-width: 800px;\n      margin: 0 auto;\n      padding: 40px;\n      background: white;\n      font-family: 'Georgia', 'Times New Roman', serif;\n      color: #333;\n      line-height: 1.6;\n    }\n    \n    .classic-cv h1 {\n      font-size: 28px;\n      text-align: center;\n      margin-bottom: 5px;\n      color: #1a1a1a;\n      font-weight: normal;\n      letter-spacing: 2px;\n      text-transform: uppercase;\n    }\n    \n    .classic-cv .contact {\n      text-align: center;\n      font-size: 13px;\n      color: #666;\n      margin-bottom: 25px;\n    }\n    \n    .classic-cv .contact span {\n      margin: 0 10px;\n    }\n    \n    .classic-cv .section {\n      margin-bottom: 25px;\n    }\n    \n    .classic-cv .section-title {\n      font-size: 14px;\n      font-weight: bold;\n      text-transform: uppercase;\n      letter-spacing: 2px;\n      border-bottom: 2px solid #333;\n      padding-bottom: 5px;\n      margin-bottom: 15px;\n      color: #1a1a1a;\n    }\n    \n    .classic-cv .summary {\n      font-style: italic;\n      text-align: center;\n      color: #555;\n      font-size: 14px;\n    }\n    \n    .classic-cv .entry {\n      margin-bottom: 15px;\n    }\n    \n    .classic-cv .entry-header {\n      display: flex;\n      justify-content: space-between;\n      align-items: baseline;\n      flex-wrap: wrap;\n    }\n    \n    .classic-cv .entry-title {\n      font-weight: bold;\n      font-size: 15px;\n    }\n    \n    .classic-cv .entry-subtitle {\n      font-style: italic;\n      color: #555;\n      font-size: 14px;\n    }\n    \n    .classic-cv .entry-date {\n      color: #666;\n      font-size: 13px;\n    }\n    \n    .classic-cv .entry-description {\n      margin-top: 5px;\n      font-size: 14px;\n    }\n    \n    .classic-cv ul {\n      margin: 5px 0 0 20px;\n      padding: 0;\n    }\n    \n    .classic-cv li {\n      margin-bottom: 3px;\n      font-size: 14px;\n    }\n    \n    .classic-cv .skills-list {\n      display: flex;\n      flex-wrap: wrap;\n      gap: 10px;\n    }\n    \n    .classic-cv .skill-item {\n      background: #f5f5f5;\n      padding: 5px 15px;\n      border-radius: 3px;\n      font-size: 13px;\n    }\n    \n    @media (max-width: 600px) {\n      .classic-cv {\n        padding: 20px;\n      }\n      .classic-cv h1 {\n        font-size: 22px;\n      }\n      .classic-cv .entry-header {\n        flex-direction: column;\n      }\n      .classic-cv .entry-date {\n        margin-top: 3px;\n      }\n    }\n    \n    @media print {\n      .classic-cv {\n        padding: 20px;\n      }\n    }\n  </title>\n</head>\n<body>\n  <div class=\"classic-cv\">\n    <h1>{{name}}</h1>\n    <div class=\"contact\">\n      <span>{{email}}</span>\n      <span>‚Ä¢</span>\n      <span>{{phone}}</span>\n      <span>‚Ä¢</span>\n      <span>{{location}}</span>\n      {{#if linkedin}}\n      <span>‚Ä¢</span>\n      <span>{{linkedin}}</span>\n      {{/if}}\n    </div>\n\n    <div class=\"section\">\n      <div class=\"section-title\">Professional Summary</div>\n      <p class=\"summary\">{{summary}}</p>\n    </div>\n\n    <div class=\"section\">\n      <div class=\"section-title\">Work Experience</div>\n      \n      {{#each experiences}}\n      <div class=\"entry\">\n        <div class=\"entry-header\">\n          <span class=\"entry-title\">{{this.title}}</span>\n          <span class=\"entry-date\">{{this.startDate}} - {{this.endDate}}</span>\n        </div>\n        <div class=\"entry-subtitle\">{{this.company}}, {{this.location}}</div>\n        <ul>\n          {{#each this.responsibilities}}\n          <li>{{this}}</li>\n          {{/each}}\n        </ul>\n      </div>\n      {{/each}}\n    </div>\n\n    <div class=\"section\">\n      <div class=\"section-title\">Education</div>\n      \n      {{#each education}}\n      <div class=\"entry\">\n        <div class=\"entry-header\">\n          <span class=\"entry-title\">{{this.degree}}</span>\n          <span class=\"entry-date\">{{this.startYear}} - {{this.endYear}}</span>\n        </div>\n        <div class=\"entry-subtitle\">{{this.school}}, {{this.location}}</div>\n        {{#if this.details}}\n        <div class=\"entry-description\">{{this.details}}</div>\n        {{/if}}\n      </div>\n      {{/each}}\n    </div>\n\n    <div class=\"section\">\n      <div class=\"section-title\">Skills</div>\n      <div class=\"skills-list\">\n        {{#each skills}}\n        <span class=\"skill-item\">{{this}}</span>\n        {{/each}}\n      </div>\n    </div>\n\n    {{#if certifications}}\n    <div class=\"section\">\n      <div class=\"section-title\">Certifications</div>\n      {{#each certifications}}\n      <div class=\"entry\">\n        <div class=\"entry-header\">\n          <span class=\"entry-title\">{{this.name}}</span>\n          <span class=\"entry-date\">{{this.year}}</span>\n        </div>\n        {{#if this.issuer}}\n        <div class=\"entry-subtitle\">{{this.issuer}}</div>\n        {{/if}}\n      </div>\n      {{/each}}\n    </div>\n    {{/if}}\n\n    {{#if languages}}\n    <div class=\"section\">\n      <div class=\"section-title\">Languages</div>\n      <div class=\"skills-list\">\n        {{#each languages}}\n        <span class=\"skill-item\">{{this.name}} - {{this.level}}</span>\n        {{/each}}\n      </div>\n    </div>\n    {{/if}}\n  </div>\n</body>\n</html>",
    "category": "Professional",
    "isActive": true,
    "createdAt": "2026-02-07T00:00:00Z",
    "updatedAt": "2026-02-07T00:00:00Z"
